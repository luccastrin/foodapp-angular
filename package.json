{
  "name": "foodapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration dev --port 4200 --host 0.0.0.0",
    "start:test": "ng serve --configuration test --port 4200 --host 0.0.0.0 --disableHostCheck=true",
    "start:test:api": "npm run sw-auto && nodemon ./api/server.js --watch api/*",
    "build": "ng build",
    "test": "jest --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:open:test": "concurrently --success first --kill-others \"npm:start:test\"  \"npm:start:test:api\" \"npm:cy:open\"",
    "sw-auto": "node api/swagger.js",
    "server": "json-server --watch db.json",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.6",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.1.6",
    "@angular/compiler": "~10.1.6",
    "@angular/core": "~10.1.6",
    "@angular/forms": "~10.1.6",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.1.6",
    "@angular/platform-browser-dynamic": "~10.1.6",
    "@angular/router": "~10.1.6",
    "concurrently": "^7.2.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.7",
    "@angular/cli": "~10.1.7",
    "@angular/compiler-cli": "~10.1.6",
    "@types/jest": "^27.4.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "cors": "^2.8.5",
    "cypress": "^9.6.1",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.1",
    "nodemon": "^2.0.16",
    "protractor": "~7.0.0",
    "swagger-autogen": "^2.21.3",
    "swagger-ui-express": "^4.4.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
